
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Index</title>
    <meta name="viewport" content="width=device-width" />

    <script src="@Url.Content("~/Scripts/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.easyui.min.js")" type="text/javascript"></script>
      
    <script src="~/Scripts/jquery.tree.js"></script>
    <link href="~/Content/tree/css/tree.css" rel="stylesheet" />

    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/themes/default/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/home")
            
    @Styles.Render("~/Content/toastr.css")
    
    @Scripts.Render("~/Scripts/knockout-2.3.0.js")
    @Scripts.Render("~/Scripts/json_eval.js")
    @Scripts.Render("~/Scripts/toastr.js")
    @Scripts.Render("~/Scripts/linq.js")

    @Scripts.Render("~/Scripts/JMWebApplication.js")
    @Scripts.Render("~/Scripts/jquery.blockUI.js")


</head>
<body class="easyui-layout">
    <div id="OverTimeLogin" class="easyui-dialog" data-options="closed:true,modal:true">
        <iframe width="726px" scrolling="no" height="497px" frameborder="0" id="iOverTimeLogin"></iframe>
    </div>

    <div data-options="region:'north',border:false " style="height: 60px;background:#B3DFDA;padding:10px">
        <div class="define-head">
            <div class="define-logo">
                <div id="LoginTopLine">System Manage</div>
                <div id="LoginBotoomLine">MVC5+EF5.0+EasyUI</div>
            </div>
            <div class="define-account">
                
            </div>
        </div>
    </div>

    <div data-options="region:'west',split:true,title:'菜单列表'" style="width: 200px; height:100%; padding-top: 2px; background-color:#fff; overflow:auto">
        <div id="RightTree" style=" background-color:#fff;">
            <div class="panel-loading">加载中...</div>
        </div>
    </div>

    <div data-options="region:'south',border:false" style="height: 50px;background:#A9FACD;padding:10px;">
        <div class="define-bottom">
           

        </div>
    </div>
    <div data-options="region:'center',border:false">
        <div id="mainTab" class="easyui-tabs" data-options="fit:true">
            <div title="我的桌面" data-options="closable:true" style="overflow: hidden; background:#fff">
                <iframe scrolling="auto" frameborder="0" src="" style="width: 100%; height: 100%;"></iframe>
            </div>
            <div  title="My Test" id="SysModuleInquiry" class="hero-unit" data-bind="visible: DisplayContent" style="display:none;">
                
                <!--====== Container ======-->
                <table id="CustomerInquiryTable" border="0" class="table" style="width: 100%;">
                    <thead>
                        
                        <tr>

                            <th style="width: 15%; height: 25px">
                                <a style="cursor: pointer; text-decoration: underline; color: black" >id </a>
                                
                            </th>
                            <th style="width: 15%; height: 25px">
                                <a style="cursor: pointer; text-decoration: underline; color: black"> Name</a>

                            </th>
                            <th style="width: 15%; height: 25px">
                                <a style="cursor: pointer; text-decoration: underline; color: black">English</a>

                            </th>
                            <th style="width: 15%; height: 25px">
                                <a style="cursor: pointer; text-decoration: underline; color: black">Parent</a>
                                ;
                            </th>
                            <th style="width: 15%; height: 25px">
                                <a style="cursor: pointer; text-decoration: underline; color: black">Url</a>

                            </th>

                        </tr>

                    </thead>
                    <tbody data-bind="foreach:SysModules">
                        <tr>
                            <td><div data-bind="text: Id"></div></td>
                            <td><div data-bind="text: Name "></div></td>
                            <td><div data-bind="text: EnglishName"></div></td>
                            <td><div data-bind="text: ParentId"></div></td>
                            <td><div data-bind="text: Url"></div></td>
                        </tr>
                    </tbody>
                </table>
                <div id="MessageBox" data-bind="html: MessageBox" style="padding-bottom:10px; padding-top:10px"></div>
            </div>
           
        </div>
    </div>
    <div id="tab_menu" class="easyui-menu" style="width: 150px;">
        <div id="tab_menu-tabrefresh" data-options="iconCls:'icon-reload'">
            刷新
        </div>
        <div id="tab_menu-openFrame">
            在新的窗体打开
        </div>
        <div id="tab_menu-tabcloseall">
            关闭所有
        </div>
        <div id="tab_menu-tabcloseother">
            关闭其他标签页
        </div>
        <div class="menu-sep">
        </div>
        <div id="tab_menu-tabcloseright">
            关闭右边
        </div>
        <div id="tab_menu-tabcloseleft">
            关闭左边
        </div>
        <div id="tab_menu-tabclose" data-options="iconCls:'icon-remove'">
            关闭
        </div>
        <div id="menu" class="easyui-menu" style="width: 150px;">
        </div>
    </div>

</body>
</html> 

<script type="text/javascript">

    var pageSize = 15;
    var sysModuleInquiryDivID = document.getElementById('SysModuleInquiry');

</script>

<script src="@Url.Content("~/ViewModels/Manage/SysModuleInquiry.js")" type="text/javascript">
</script>


 